menu "Example Configuration"

    config EXAMPLE_SCAN_LIST_SIZE
        int "Max size of scan list"
        range 0 20
        default 10
        help
            The size of array that will be used to retrieve the list of access points.

    config EXAMPLE_WIFI_SSID
        string "WiFi Access Point SSID"
        default "test"
        help
            Set WiFi AP SSID.

    config EXAMPLE_WIFI_PASS
        string "WiFi Access Point password"
        default "test"
        help
            Set WiFi AP password.

    config FLASH_LED_PIN
        int "Flash LED pin"
        range 0 10
        default 4
        help
            Flash LED pin number.

endmenu
